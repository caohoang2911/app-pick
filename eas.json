{
  "build": {
    "dev": {
      "node": "22.9.0",
      "autoIncrement": true,
      "developmentClient": false,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "prebuildCommand": "./commands/prebuild.sh"
      },
      "ios": {
        "distribution": "store",
        "image": "macos-sonoma-14.6-xcode-16.1",
        "resourceClass": "m-medium",
        "prebuildCommand": "./commands/prebuild.sh",
        "simulator": false,
        "env": {
          "COCOAPODS_VERSION": "1.16.2"
        }
      },
      "channel": "development",
      "env": {
        "EAS_BUILD_PROFILE": "dev"
      }
    },
    "prod": {
      "node": "22.9.0",
      "autoIncrement": true,
      "developmentClient": false,
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "prebuildCommand": "./commands/prebuild.sh"
      },
      "ios": {
        "distribution": "store",
        "image": "macos-sonoma-14.6-xcode-16.1",
        "resourceClass": "m-medium",
        "prebuildCommand": "./commands/prebuild.sh",
        "env": {
          "COCOAPODS_VERSION": "1.16.2"
        }
      },
      "channel": "production",
      "env": {
        "EAS_BUILD_PROFILE": "prod"
      }
    }
  },
  "submit": {
