{
  "expo": {
    "name": "App Pick",
    "slug": "seedcom-app-pick",
    "scheme": "apppick",
    "version": "1.0.16",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.caohoang2911.seedcom-app-pick",
      "associatedDomains": [
        "applinks:expo.dev",
        "applinks:oms.seedcom.vn"
      ],
      "googleServicesFile": "./GoogleService-Info.plist",
      "buildNumber": "69",
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "Ứng dụng cần quyền truy cập mạng cục bộ để kết nối với máy in",
        "NSBonjourServices": ["_http._tcp"],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "apppick"
            ]
          }
        ],
        "UIBackgroundModes": ["remote-notification"]
      },
      "sounds": ["./assets/sounds/ding.mp3"],
      "minimumVersion": "15.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.caohoang2911.AppPick",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "oms.seedcom.vn",
              "pathPrefix": "/order-detail"
            },
            {
              "scheme": "https",
              "host": "oms.seedcom.vn",
              "pathPrefix": "/order-invoice"
            },
            {
              "scheme": "https",
              "host": "oms.seedcom.vn",
              "pathPrefix": "/scan-to-delivery"
            },
            {
              "scheme": "https",
              "host": "oms.seedcom.vn",
              "pathPrefix": "/orders"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications"
      ],
      [
        "expo-font",
        {
          "fonts": [
            "node_modules/@expo-google-fonts/inter/Inter_900Black.ttf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      [
        "expo-asset",
        {
          "assets": [
            "./assets/logo.png"
          ]
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/auth",
      "@react-native-firebase/crashlytics",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "podfileProperties": {
              "use_modular_headers!": true
            }
          }
        }
      ],
      "expo-router",
      "with-react-native-tcp-socket"
    ],
    "extra": {
      "eas": {
        "projectId": "58d442de-186f-4ce5-a7b4-664b666f2ac0"
      }
    },
    "runtimeVersion": "1.0.2",
    "updates": {
      "fallbackToCacheTimeout": 0,
      "checkAutomatically": "NEVER",
      "url": "https://u.expo.dev/58d442de-186f-4ce5-a7b4-664b666f2ac0"
    }
  }
}
