{
  "name": "seedcom-app-pick",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo run:android --no-build-cache --device",
    "ios": "expo run:ios --no-build-cache --device",
    "web": "expo start --web",
    "export": "expo export --platform web",
    "prebuild": "expo prebuild",
    "pod": "npx pod-install",
    "clean-ios": "expo prebuild --platform ios  --clean",
    "clean-android": "expo prebuild --platform android  --clean",
    "serve": "npx serve -s dist",
    "build-development": "eas build -p android --profile development",
    "build-production": "eas build -p android --profile production",
    "build-ios": "eas build -p ios --profile preview",
    "build-prod": "eas build --profile production --platform ios --auto-submit",
    "build-dev": "eas build --profile development --platform ios --auto-submit",
    "build-android": "eas build:run -p android --lastest",
    "build-android-local": "eas build --platform android --local",
    "codepush-preview": "eas update --profile=preview",
    "codepush-dev": "eas update --profile=development",
    "codepush-prod": "eas update --profile=production",
    "list_build": "eas build --list"
  },
  "dependencies": {
