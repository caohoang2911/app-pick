{
  "name": "seedcom-app-pick",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start:dev": "cross-env EAS_BUILD_PROFILE=dev expo start --dev-client",
    "start:prod": "cross-env EAS_BUILD_PROFILE=prod expo start --no-dev --minify --dev-client",
    "android:dev": "cross-env EAS_BUILD_PROFILE=dev expo run:android --no-build-cache --device",
    "android:prod": "cross-env EAS_BUILD_PROFILE=prod expo run:android --no-build-cache --device --port 8082",
    "ios": "expo run:ios --no-build-cache --device",
    "ios:dev": "cross-env EAS_BUILD_PROFILE=dev expo run:ios --no-build-cache --device",
    "ios:prod": "cross-env EAS_BUILD_PROFILE=prod expo run:ios --no-build-cache",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "prebuild:ios:dev": "cross-env EAS_BUILD_PROFILE=dev bash ./commands/copy-google-services.sh && cross-env EAS_BUILD_PROFILE=dev expo prebuild --platform ios",
    "prebuild:ios:prod": "cross-env EAS_BUILD_PROFILE=prod bash ./commands/copy-google-services.sh && cross-env EAS_BUILD_PROFILE=prod expo prebuild --platform ios",
    "prebuild:android:dev": "cross-env EAS_BUILD_PROFILE=dev bash ./commands/copy-google-services.sh && cross-env EAS_BUILD_PROFILE=dev expo prebuild --platform android",
    "prebuild:android:prod": "cross-env EAS_BUILD_PROFILE=prod bash ./commands/copy-google-services.sh && cross-env EAS_BUILD_PROFILE=prod expo prebuild --platform android",
    "build:android:dev": "eas build --platform android --profile dev",
    "build:android:prod": "eas build --platform android --profile prod",
    "build:android:dev:local": "eas build --platform android --profile dev --local --clear-cache",
    "build:android:prod:local": "eas build --platform android --profile prod --local --clear-cache",
    "build:ios:dev": "eas build --platform ios --profile dev --auto-submit",
    "build:ios:prod": "eas build --platform ios --profile prod --auto-submit",
    "build:ios:dev:local": "eas build --platform ios --profile dev --local --clear-cache",
    "build:ios:prod:local": "eas build --platform ios --profile prod --local --clear-cache",
    "list:build": "eas build:list",
    "resubmit:ios:dev": "eas submit --platform ios --profile dev --latest --verbose",
    "codepush:version": "node scripts/codepush-version.js",
    "codepush:dev": "yarn codepush:version && eas update --branch development --message 'Update CodePush version'",
    "codepush:prod": "yarn codepush:version && eas update --branch production --message 'Update CodePush version'",
    "env:dev": "node scripts/setup-env.js dev",
    "env:prod": "node scripts/setup-env.js prod",
    "pod": "npx pod-install",
    "export": "expo export --platform web",
    "serve": "npx serve -s dist",
    "reset": "watchman watch-del-all && rm -rf node_modules .expo .expo-shared ios/build ios/Pods ios/Podfile.lock android/build android/app/build package-lock.json yarn.lock && rm -rf ~/Library/Developer/Xcode/DerivedData && yarn install && cd ios && pod install && cd ..",
    "postinstall": "patch-package",
    "android": "expo run:android"
  },
  "dependencies": {
    "@dev-plugins/react-navigation": "^0.0.6",
    "@dev-plugins/react-query": "^0.0.6",
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/metro-runtime": "~3.2.1",
    "@expo/vector-icons": "^14.0.2",
    "@gorhom/bottom-sheet": "^4.6.4",
    "@gorhom/portal": "^1.0.14",
    "@hookform/resolvers": "^3.9.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@react-native-firebase/app": "17.5.0",
    "@react-native-firebase/auth": "17.5.0",
    "@react-native-firebase/crashlytics": "17.5.0",
    "@react-native-firebase/messaging": "17.5.0",
    "@react-navigation/drawer": "^6.7.2",
    "@tanstack/react-query": "^5.51.21",
    "axios": "^1.7.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "expo": "^51.0.22",
    "expo-asset": "~10.0.10",
    "expo-build-properties": "~0.12.5",
    "expo-camera": "~15.0.14",
    "expo-dev-client": "~4.0.29",
    "expo-image": "~1.12.13",
    "expo-image-manipulator": "~12.0.5",
    "expo-linking": "~6.3.1",
    "expo-network": "~6.0.1",
    "expo-notifications": "~0.28.15",
    "expo-radio-button": "^1.0.8",
    "expo-router": "^3.5.18",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.22",
    "formik": "^2.4.6",
    "lodash": "^4.17.21",
    "lucide-react": "^0.414.0",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.46",
    "nativewind": "^4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.52.1",
    "react-native": "0.74.5",
    "react-native-avoid-softinput": "^4.0.1",
    "react-native-element-dropdown": "^2.12.1",
    "react-native-flash-message": "^0.4.2",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-image-picker": "^8.2.1",
    "react-native-mmkv": "^2.12.2",
    "react-native-qrcode-svg": "^6.3.12",
    "react-native-reanimated": "3.15.0",
    "react-native-safe-area-context": "4.10.5",
    "react-native-screens": "^3.32.0",
    "react-native-svg": "15.2.0",
    "react-native-tcp-socket": "^6.2.0",
    "react-native-view-shot": "3.8.0",
    "react-native-web": "~0.19.10",
    "react-native-webview": "13.8.6",
    "react-native-worklets": "^0.6.0",
    "react-query-kit": "^3.3.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7",
    "with-react-native-tcp-socket": "^1.0.1",
    "yup": "^1.4.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@types/lodash": "^4.17.7",
    "@types/react": "~18.2.45",
    "cross-env": "^10.0.0",
    "patch-package": "^8.0.0",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.1.3"
  },
  "private": true
}
